.contain {
  position: absolute;
  top: 350rpx;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

ec-canvas {
  width: 100%;
  height: 100%;
}

.pan-choose {
  display: flex;
  padding: 20rpx;
  align-items: center;
  justify-content: space-between;
}

.picker {
  border: 1px solid #dedede;
  padding: 10rpx;
  background: skyblue;
  border-radius: 12rpx;
  font-weight: bold;
  color: #666;
}

.selectlist {
  position: relative;
  z-index: 999999;
}

.nowdate {
  font-size: 15px;
  font-weight: bold;
}

.centerlist{
  padding:10rpx;
  text-align:center;
}

.date-panel{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
}
